{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/freelancers/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/freelancers",
        "main": "apps/freelancers/src/main.ts",
        "outputFileName": "index.js",
        "tsConfig": "apps/freelancers/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "externalDependencies": "none",
          "fileReplacements": [
            {
              "replace": "apps/freelancers/src/environments/environment.ts",
              "with": "apps/freelancers/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "dependsOn": [
        {
          "params": "forward",
          "target": "build",
          "projects": "self"
        }
      ],
      "options": {
        "command": "zip -qj dist/apps/freelancers/package.zip dist/apps/freelancers/index.js"
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "freelancers:build"
      },
      "dependsOn": [
        {
          "target": "migration:run",
          "params": "forward",
          "projects": "dependencies"
        }
      ],
      "configurations": {
        "production": {
          "buildTarget": "freelancers:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/freelancers/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/freelancers"
      ],
      "options": {
        "jestConfig": "apps/freelancers/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "implicitDependencies": [
    "postgres-db"
  ],
  "tags": []
}
