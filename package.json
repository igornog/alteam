  "license": "MIT",
  "scripts": {
    "start": "npx nx serve front",
    "all": "npx nx run-many --target=serve --projects=$(ls -d apps/*/ | tr -d / | paste -sd ',') --parallel=$(ls -d apps/*/ | wc -l | tr -d ' ')",
    "storybook": "npx nx run front:storybook",
    "build-storybook": "npx nx run front:build-storybook",
    "chromatic": "npx chromatic --force-rebuild",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json,md}'",
    "check": "prettier --check './**/*.{js,jsx,ts,tsx,json,md}'",
    "deploy:base": "nx deploy infrastructure  --configuration=production  --output-style stream",
    "deploy": "npm run deploy:base && nx run-many --target=deploy --configuration=production --all --exclude=infrastructure --output-style stream",
    "fix-lint-errors": "eslint --fix ."
  },
